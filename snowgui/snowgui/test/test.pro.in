QT       += core gui widgets

TARGET = test
TEMPLATE = app

# Directories for ICE and the astrophotography library
ICEDIR = @ICEDIR@
ICELIBS = @ICELIBS@
ASTROPATH = @ASTROPATH@
ASTROLIBDIR = @ASTROLIBDIR@

LIBS += -L../common -lcommon 
LIBS += -L$${ASTROLIBDIR} -lastro -liceastro
LIBS +=	$${ICELIBS} -lIce -lIceUtil

INCLUDEPATH += /usr/local/include
INCLUDEPATH += $${ASTROPATH}/include
INCLUDEPATH += $${ICEDIR}/include
INCLUDEPATH += ../common 

ASTROGUISOURCES =

ASTROGUIHEADERS =

FORMS    +=								\

SOURCES += imagedisplaytest.cpp $${ASTROGUISOURCES}

HEADERS  += $${ASTROGUIHEADERS}

